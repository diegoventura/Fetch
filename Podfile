platform :ios, '9.0'
use_frameworks!

def shared
    pod 'Alamofire', '~> 4.4'
    pod 'AlamofireImage', '~> 3.1'
    pod 'SwiftyJSON', '~> 3.1.4'
    pod 'KeychainAccess', '~> 3.0.2'
    pod 'Realm'
    pod 'RealmSwift'
    pod 'Downpour', '~> 0.2.0'
end

target 'Fetch' do
    shared
    pod 'MZFormSheetPresentationController', '~> 2.4.2'
    pod '1PasswordExtension'
    pod 'TUSafariActivity'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'ReachabilitySwift', '~> 3'
end

target 'PutioKit' do
  shared
end
